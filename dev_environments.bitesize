project: s3test
environments:
  - name: dev
    namespace: s3-test-int
    deployment:
      method: rolling-upgrade
    services:
      - name: eosnstestkush
        type: sns
        version: 1.0.0
        replicas: 1
        options:
           endpoint: 'kushanbandara.vijithbandara@pearson.com'
           protocol: 'email'
      - name: eosnstestkush
        type: sqs
        version: 1.0.0
        replicas: 1
      - name: eosnstestkush
        type: s3
        options:
          bucket_region: "eu-west-1"
          bucket_encryption: "aes-256"
